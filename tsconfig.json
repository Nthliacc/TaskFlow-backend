{
    "compilerOptions": {
      "target": "es5",
      "module": "CommonJS",
      "outDir": "./functions",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  